        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  CASE\n    WHEN a.action IN ('approve_user', 'reject_user', 'promote_to_admin', 'suspend_user', 'unsuspend_user', 'generate_password_reset_token') THEN 'user_management'\n    WHEN a.action IN ('delete_post', 'hard_delete_post', 'restore_post', 'delete_comment', 'hard_delete_comment', 'restore_comment', 'update_post', 'update_comment') THEN 'content_moderation'\n    WHEN a.action IN ('toggle_link_metadata', 'toggle_mfa_requirement', 'update_section', 'delete_section') THEN 'settings'\n    WHEN a.action IN ('enroll_mfa', 'enable_mfa', 'disable_mfa') THEN 'security'\n    ELSE 'other'\n  END AS \"Category\",\n  COUNT(*) AS \"Actions\"\nFROM audit_logs a\nJOIN users admin ON admin.id = a.admin_user_id AND admin.is_admin = true\nWHERE $__timeFilter(a.created_at)\n  AND a.admin_user_id IS NOT NULL\n  AND (${admin_user:sqlstring} = '' OR a.admin_user_id::text = ${admin_user:sqlstring})\nGROUP BY 1\nORDER BY \"Actions\" DESC",
          "refId": "A"
        }
      ],
      "title": "Actions by Category",
      "type": "piechart"
    },
